container_commands:
  01-migrate-env:
    command: "mv -f .env.aws .env"
    cwd: "/var/app/ondeck"
    leader_only: true
  02-artisan_migrate:
    command: "php artisan migrate --force"
    cwd: "/var/app/ondeck"
    leader_only: true
  03-passport_keys:
    command: "php artisan passport:keys"
    cwd: "/var/app/ondeck"
    leader_only: true